{
  "name": "portfolio",
  "version": "1.11.3",
  "description": "My personal portfolio project.",
  "repository": "git@github.com:matthieu-locussol/portfolio.git",
  "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run next dev",
    "build": "yarn run next build",
    "start": "yarn run next start",
    "export": "yarn run next export",
    "postinstall": "yarn husky install",
    "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "check:lint": "biome check *",
    "tests": "jest",
    "tests:ci": "jest --ci",
    "release": "standard-version -a"
  },
  "lint-staged": {
    "**/*.{ts,tsx,d.ts}": [
      "yarn check:lint"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "next": "^12.0.9",
    "next-pwa": "^5.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "tsc-files": "^1.1.3",
    "typescript": "^4.5.5"
  }
}

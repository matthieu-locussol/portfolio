{
   "name": "portfolio",
   "version": "1.1.0",
   "description": "My personal portfolio project.",
   "repository": "git@github.com:matthieu-locussol/portfolio.git",
   "author": "Matthieu Locussol <dev.matthieu.locussol@gmail.com>",
   "license": "MIT",
   "scripts": {
      "dev": "yarn run next dev",
      "build": "yarn run next build",
      "start": "yarn run next start",
      "export": "yarn run next export",
      "gitignore": "curl -sL https://www.toptal.com/developers/gitignore/api/nextjs,node,react,reactnative,yarn,visualstudiocode > .gitignore",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json}\"",
      "check:eslint": "eslint --ignore-path .gitignore src/**/*",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "tests": "jest",
      "tests:ci": "jest --ci",
      "release": "standard-version -a"
   },
   "lint-staged": {
      "*.{ts,tsx,d.ts}": [
         "tsc-files --noEmit",
         "yarn check:eslint"
      ]
   },
   "dependencies": {
      "@emotion/react": "^11.7.1",
      "@emotion/styled": "^11.6.0",
      "@mui/icons-material": "^5.2.5",
      "@mui/material": "^5.2.5",
      "next": "^12.0.7",
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
   },
   "devDependencies": {
      "@commitlint/cli": "^15.0.0",
      "@commitlint/config-conventional": "^15.0.0",
      "@types/jest": "^27.0.3",
      "@types/react": "^17.0.38",
      "@types/react-dom": "^17.0.11",
      "@typescript-eslint/eslint-plugin": "^5.8.0",
      "@typescript-eslint/parser": "^5.8.0",
      "eslint": "^8.5.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-airbnb-typescript": "^16.1.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "husky": "^7.0.4",
      "jest": "^27.4.5",
      "lint-staged": "^12.1.4",
      "prettier": "^2.5.1",
      "pretty-quick": "^3.1.3",
      "standard-version": "^9.3.2",
      "ts-jest": "^27.1.2",
      "tsc-files": "^1.1.3",
      "typescript": "^4.5.4"
   }
}
